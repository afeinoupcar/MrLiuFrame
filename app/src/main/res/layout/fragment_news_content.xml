<?xml version="1.0" encoding="utf-8"?>
<com.cjj.MaterialRefreshLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/newscontent_fra_mrl"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:overlay="true"
    app:progress_colors="@array/material_colors"
    app:progress_size_type="normal"
    app:wave_height_type="normal"
    app:wave_show="false"
    app:isLoadMore="true">
   <android.support.v7.widget.RecyclerView
       android:id="@+id/newscontent_fra_rv"
       android:layout_width="match_parent"
       android:layout_height="match_parent"
       android:layout_marginBottom="50dp"></android.support.v7.widget.RecyclerView>
</com.cjj.MaterialRefreshLayout>
